{
    "name": "NodeInvoiceBoilerplate",
    "version": "1.0.0",
    "description": "NodeInvoiceBoilerplate",
    "main": "build/index.js",
    "scripts": {
        "build": "rimraf ./build && tsc",
        "start:dev": "nodemon",
        "start": "npm run build && node build/index.js",
        "sequelize:migration:generate": "sequelize-cli --options-path ./.sequelize-${database} --env ${database} migration:generate",
        "sequelize:db:migrate": "sequelize-cli --options-path ./.sequelize-${database} --env ${database} db:migrate",
        "sequelize:db:migrate:undo": "sequelize-cli --options-path ./.sequelize-${database} --env ${database} db:migrate:undo",
        "sequelize:db:migrate:undo:all": "sequelize-cli --options-path ./.sequelize-${database} --env ${database} db:migrate:undo:all",
        "sequelize:db:seed:all": "sequelize-cli --options-path ./.sequelize-${database} --env ${database} db:seed:all",
        "test": "mocha -r ts-node/register './src/**/*.spec.ts'",
        "test:watch": "mocha -r ts-node/register './src/**/*.spec.ts' --watch --watch-files './src/**/*.spec.ts'"
    },
    "author": "Ot",
    "license": "MIT",
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.0.0",
        "@types/pg": "^8.6.5",
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "eslint": "^8.13.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^2.7.1",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-webpack-plugin": "^3.1.1",
        "nodemon": "^2.0.15",
        "prettier": "^2.6.0",
        "prettier-eslint": "^13.0.0",
        "ts-sinon": "^2.0.2"
    },
    "dependencies": {
        "@google-cloud/pubsub": "^4.0.2",
        "@types/cors": "^2.8.12",
        "@types/gapi": "^0.0.42",
        "@types/lodash": "^4.14.182",
        "@types/mailgun-js": "^0.22.12",
        "@types/node-cron": "^3.0.1",
        "@types/passport-jwt": "^3.0.7",
        "@types/socket.io": "^3.0.2",
        "aws-sdk": "^2.1148.0",
        "axios": "^0.26.1",
        "bcrypt": "^5.1.0",
        "chai": "^4.3.7",
        "date-fns": "^2.28.0",
        "date-fns-tz": "^1.3.4",
        "dotenv": "^16.0.0",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "express-fileupload": "^1.4.0",
        "form-data": "^4.0.0",
        "googleapis": "^100.0.0",
        "helmet": "^6.0.1",
        "http-status": "^1.5.0",
        "joi": "^17.6.0",
        "lodash": "^4.17.21",
        "mailgun.js": "^6.0.1",
        "mocha": "^10.1.0",
        "moment": "^2.30.1",
        "moment-timezone": "^0.5.45",
        "mysql2": "^2.3.3",
        "node-cron": "^3.0.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "puppeteer": "^21.0.2",
        "redis": "^4.3.1",
